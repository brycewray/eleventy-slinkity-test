---
layout: base
tags:
  - nav
navtitle: Posts
title: Posts
description: Posts for this website
eleventyExcludeFromCollections: true
pagination: 
  data: collections.post
  size: 5
  reverse: true
  alias: posts
---

<main>
  <div class="container ctr sansSerif">
    <h1 class="posts-Head">Posts</h1>
    <div>
      <p class="posts-pagerThingP">
        {% if pagination.firstPageHref != page.url %}
        <a class="icon" aria-label="First page" href="{{ pagination.firstPageHref}}">&#9664;&#9664;</a>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a class="icon" aria-label="Previous page" href="{{ pagination.previousPageHref }}">&#9664;</a>
        {% else %}
        <span class="posts-pagerGray">&#9664;&#9664;</span>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <span class="posts-pagerGray">&#9664;</span>
        {% endif %}
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        {% if pagination.lastPageHref != page.url %}
        <a class="icon" aria-label="Next page" href="{{ pagination.nextPageHref }}">&#9654;</a>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a class="icon" aria-label="Next page" href="{{ pagination.lastPageHref }}">&#9654;&#9654;</a>
        {% else %}
        <span class="posts-pagerGray">&#9654;</span>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <span class="posts-pagerGray">&#9654;&#9654;</span>
        {% endif %}
      </p>
      <hr class="paginatorTop" />
      {% for post in posts %}
        <div>          
          <h2 class="posts-Title"><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
          <p class="posts-Subtitle">{{ post.data.subtitle }}</h2>
          <p class="posts-Dates">
            <time datetime="{{ post.date | htmlDateString }}"><strong>{{ post.date | htmlDateString }}</strong></time>
            {% if post.data.lastmod %}
            <br />Last modified <time datetime="{{ post.date | htmlDateString }}">{{ post.data.lastmod | htmlDateString }}</time>
            {% endif %}
          </p>
          <p class="posts-Description">
            {{ post.data.description }}
          </p>
        </div>
      {% endfor %}
      <hr class="paginatorBottom" />
      <p class="posts-pagerThingP">
        {% if pagination.firstPageHref != page.url %}
        <a class="icon" aria-label="First page" href="{{ pagination.firstPageHref}}">&#9664;&#9664;</a>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a class="icon" aria-label="Previous page" href="{{ pagination.previousPageHref }}">&#9664;</a>
        {% else %}
        <span class="posts-pagerGray">&#9664;&#9664;</span>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <span class="posts-pagerGray">&#9664;</span>
        {% endif %}
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        {% if pagination.lastPageHref != page.url %}
        <a class="icon" aria-label="Next page" href="{{ pagination.nextPageHref }}">&#9654;</a>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a class="icon" aria-label="Next page" href="{{ pagination.lastPageHref }}">&#9654;&#9654;</a>
        {% else %}
        <span class="posts-pagerGray">&#9654;</span>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <span class="posts-pagerGray">&#9654;&#9654;</span>
        {% endif %}
      </p>
		</div>
	</div>

</main>