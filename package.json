{
  "name": "eleventy_solo_starter_njk",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf _site && rimraf src/imgout && rimraf img",
    "start": "NODE_ENV=development ELEVENTY_ENV=development TAILWIND_MODE=watch npm-run-all clean dev:slinkity",
    "dev:slinkity": "slinkity --quiet --serve --incremental",
    "build": "NODE_ENV=production ELEVENTY_ENV=production npm-run-all clean --parallel prod:slinkity",
    "prod:slinkity": "slinkity",
    "testbuild:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode --port 3000 --ui-port 3001",
    "testbuild": "npm run build && npm run testbuild:bsync"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "> 1%",
    "maintained node versions"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.5",
    "@11ty/eleventy-img": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "fast-glob": "^3.2.7",
    "glob": "^7.2.0",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "luxon": "^2.1.1",
    "markdown-it": "^12.2.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.2.1",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "rimraf": "^3.0.2",
    "slinkity": "^0.4.0",
    "tailwindcss": "^3.0.0-alpha.2",
    "tailwindcss-logical": "^2.0.0"
  }
}
